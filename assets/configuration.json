{
  "planets": [
    { "name": "Mars", "value": "mars" },
    { "name": "Earth", "value": "earth" },
    { "name": "The Moon", "value": "moon" }
  ],
  "MapProject": {
    "description": "",
    "help": "",
    "pages": [
      {
        "components": [
          {
            "param": "sensor",
            "title": "Orbital Spacecraft Sensor",
            "help": "mapproject",
            "type": "radiolist",
            "options": {
              "items": [
                { "name": "Mars MRO CTX", "value": "ctx", "planet": "Mars" },
                { "name": "Mars MRO CTX1", "value": "ctx1", "planet": "Mars" },
                { "name": "Mars MRO CTX2", "value": "ctx2", "planet": "Mars" },
                { "name": "Mars MRO CTX3", "value": "ctx3", "planet": "Mars" },
                { "name": "Mars MRO CTX4", "value": "ctx4", "planet": "Mars" },
                { "name": "Mars MRO CTX5", "value": "ctx5", "planet": "Mars" },
                { "name": "Mars MRO CTX6", "value": "ctx6", "planet": "Mars" },
                {
                  "name": "Mars MRO HiRISE",
                  "value": "hirise",
                  "planet": "Mars"
                }
              ]
            }
          },
          {
            "title": "Need Help Finding PDS Images for Projection?",
            "type": "title"
          },
          {
            "param": "rawimg",
            "title": "FULL Local Directory Path to Input PDS Images",
            "type": "filepath"
          },
          {
            "param": "pathraw",
            "title": "PDS Input Image Filename",
            "type": "filename",
            "options": {
              "extensions": [{ "name": "PDS Images", "value": "*.IMG" }]
            }
          },
          {
            "param": "outpre",
            "title": "PREFIX Filename for Output Products",
            "type": "string"
          },
          {
            "param": "mpix",
            "title": "OUTPUT PIXEL SIZE (meters/pixel)",
            "type": "number"
          }
        ]
      },
      {
        "components": [
          {
            "param": "demimg",
            "title": "FULL Directory Path/Filename to Encompassing DEM",
            "type": "filepathname"
          },
          {
            "param": "mapref",
            "title": "Output Map Projections",
            "type": "radiolist",
            "options": {
              "items": [
                { "name": "Latitude-Longitude", "value": "latitude-longitude" },
                {
                  "name": "Equidistant",
                  "value": "equidistant"
                },
                {
                  "name": "North Pole Stereographic",
                  "value": "north_polar_stereographic"
                }
              ]
            }
          },
          {
            "param": "debug",
            "title": "Keep Temporary Files",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "title": "Other Default Parameters",
            "type": "multitext",
            "options": {
              "items": [
                { "param": "hiband", "value": "1" },
                { "param": "pathout", "value": "finalmos" },
                { "param": "minh", "value": -8528 },
                { "param": "maxh", "value": 21266 },
                {
                  "param": "mdeg",
                  "value": 59525.0,
                  "comment": "Mars Meters/Degree"
                },
                {
                  "param": "ldeg",
                  "value": 30322.2,
                  "comment": "Luna Meters/Degree"
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "CoRegister": {
    "description": "",
    "help": "",
    "pages": [
      {
        "components": [
          {
            "param": "refimg",
            "title": "MASTER Image Full Path/Filename",
            "type": "filepathname"
          },
          {
            "param": "secimg",
            "title": "SECONDARY Full Path/Filename (Image to be Stacked)",
            "type": "filepathname"
          },
          {
            "param": "outpre",
            "title": "PREFIX Filename for Output Products",
            "type": "string",
            "options": {
              "forbid": [" "],
              "maxLength": 30
            }
          },
          {
            "param": "debug",
            "title": "Keep Temporary Files",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "title": "Other Default Parameters",
            "type": "multitext",
            "options": {
              "items": [
                { "param": "pathout", "value": "finalmos" },
                { "param": "maxerr", "value": 1.0 },
                { "param": "fftgrd", "value": 48 },
                {
                  "param": "fftmag",
                  "value": 4
                },
                {
                  "param": "magmin",
                  "value": 22
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "Mosaic": {
    "description": "",
    "help": "",
    "pages": [
      {
        "components": [
          {
            "title": "Need Help Finding Map Projected Images?",
            "help": "mosaic-finding",
            "type": "title"
          },
          {
            "param": "pathraw",
            "title": "FULL PATH to Directory of ALL Input Map Images",
            "help": "mosaic-pathraw",
            "type": "filepath"
          },
          {
            "param": "outpre",
            "title": "PREFIX Filename for Output Products",
            "help": "mosaic-outpre",
            "type": "string",
            "options": {
              "forbid": [" "],
              "maxLength": 30
            }
          },
          {
            "param": "mpix",
            "title": "OUTPUT PIXEL SIZE (meters/pixel)",
            "help": "mosaic-mpix",
            "type": "number"
          },
          {
            "param": "matchbase",
            "title": "MATCH Mosaic to an existing orthobase?",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "param": "lsat",
            "title": "IF Yes, Specifiy Complete path/directory/filename",
            "type": "filepathname",
            "indent": true
          },
          {
            "param": "coverref",
            "title": "Output Projection Reference (Use this Image's Projection)",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "param": "refmap",
            "title": "IF Yes, Specifiy Complete path/directory/filename",
            "type": "filepathname",
            "indent": true
          }
        ]
      },

      {
        "components": [
          {
            "param": "imgchk",
            "title": "OVERRIDE Image Verification Warning?",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "param": "debug",
            "title": "Keep Temporary/Intermediate Files?",
            "type": "boolean",
            "options": {
              "trueText": "Yes (1)",
              "falseText": "No (0)",
              "default": false
            }
          },
          {
            "title": "Other Default Parameters",
            "type": "multitext",
            "options": {
              "items": [
                { "param": "unique", "value": "JOB1_" },
                { "param": "pathxy", "value": "xydir" },
                { "param": "pathout", "value": "finalmos" },
                {
                  "param": "topo",
                  "value": "opt/afids/data/vdev/etop02nobath.hlf",
                  "comment": "Earth Land/Mask with positive elevations 1-30000m"
                },
                {
                  "param": "deltazthresh2",
                  "value": 100.0
                },
                { "param": "edgexyth", "value": 70.0 },
                { "param": "minnumzval", "value": 4 },
                { "param": "waterthresh", "value": 5 },
                { "param": "watercount", "value": 12 },
                { "param": "getzvalwin", "value": 5 },
                { "param": "mooreopt", "value": 9.0 },
                { "param": "nah", "value": 1000 },
                { "param": "nav", "value": 1000 },
                { "param": "mars_gridspace", "value": 0.004 },
                {
                  "param": "marslatlon",
                  "value": "/data/library/projdef/mars_geographic.prj"
                },
                { "param": "mdeg", "value": 59525.0 },
                { "param": "earth_gridspace", "value": 0.004 },
                {
                  "param": "earthlatlon",
                  "value": "/data/library/projdef/earth_geographic.prj"
                },
                { "param": "edeg", "value": 108000.0 },
                { "param": "luna_gridspace", "value": "0.004" },
                {
                  "param": "lunalatlon",
                  "value": "/data/library/projdef/luna_geographic.prj"
                },
                { "param": "ldeg", "value": 30322.2 }
              ]
            }
          },
          {
            "title": "LAT/LON Output Sub-Area Box",
            "type": "multitext",
            "options": {
              "items": [
                {
                  "param": "lnpminlon",
                  "value": 999,
                  "comment": "Left Longitude Degrees; 999=No sub-area"
                },
                {
                  "param": "lnpminlat",
                  "value": "",
                  "comment": "Bottom Latitude Degrees"
                },
                {
                  "param": "lnpmaxlon",
                  "value": "",
                  "comment": "Right Longitude Degrees"
                },
                {
                  "param": "lnpmaxlat",
                  "value": "",
                  "comment": "Top Latitude Degrees"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}
